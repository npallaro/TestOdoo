        <!-- Articolo: Spese di Fatturazione -->
        <record id="product_billing_fee" model="product.product">
            <field name="name">Spese di Fatturazione</field>
            <field name="type">service</field>
            <field name="categ_id" ref="product.product_category_all"/>
